{
    "name": "ghost-cypress",
    "version": "1.0.0",
    "description": "Pruebas E2E a la plataforma GHOST con el framework Cypress.",
    "scripts": {
        "cypress:open": "cypress open --e2e --browser chrome",
        "cypress:run": "cypress run --e2e --browser chrome --headless --spec 'cypress/e2e/example'",
        "test": "npm run cypress:run",
        "format": "prettier --write cypress"
    },
    "keywords": [
        "Cypress",
        "E2E",
        "testing",
        "Ghost"
    ],
    "author": "Juan Manuel González Garzón",
    "license": "MIT",
    "dependencies": {
        "cypress": "^11.0.1"
    },
    "devDependencies": {
        "typescript": "^4.8.4"
    }
}
